{
  "name": "hospital-alert-system",
  "main": "index.js",
  "version": "2.0.0",
  "scripts": {
    "// === MAIN COMMANDS (UNIFIED) ===": "",
    "start": "./scripts/start-unified.sh network",
    "start:local": "./scripts/start-unified.sh local",
    "start:tunnel": "./scripts/start-unified.sh tunnel",
    "start:oauth": "./scripts/start-unified.sh oauth",
    "android": "EXPO_GO=1 expo start --android --host lan --go",
    "ios": "EXPO_GO=1 expo start --ios --host lan --go",
    "web": "EXPO_GO=1 expo start --web --go",
    "// === ENVIRONMENT-SPECIFIC (LOCAL=DOCKER, DEV=NEON) ===": "",
    "local": "APP_ENV=local EXPO_GO=1 expo start --host lan --go",
    "local:healthcare": "APP_ENV=local ./scripts/start-with-healthcare.sh",
    "local:oauth": "./scripts/start-local-oauth.sh",
    "oauth:fix": "./scripts/fix-oauth-local.sh",
    "local:tunnel": "APP_ENV=local EXPO_GO=1 NODE_ENV=development expo start --tunnel --go",
    "dev": "APP_ENV=development EXPO_GO=1 expo start --host lan --go",
    "dev:healthcare": "APP_ENV=development ./scripts/start-with-healthcare.sh",
    "dev:tunnel": "APP_ENV=development EXPO_GO=1 NODE_ENV=development expo start --tunnel --go",
    "staging": "APP_ENV=staging EXPO_GO=1 expo start --host lan --go",
    "start:healthcare": "./scripts/start-with-healthcare.sh",
    "// === DEVELOPMENT BUILD MODE ===": "",
    "start:dev": "expo start --host lan",
    "start:tunnel:dev": "NODE_ENV=development expo start --tunnel",
    "android:dev": "expo start --android --host lan",
    "ios:dev": "expo start --ios --host lan",
    "web:dev": "DATABASE_URL=$NEON_DATABASE_URL expo start --web --host lan",
    "// === DATABASE OPERATIONS ===": "",
    "db:local:up": "docker-compose -f docker-compose.local.yml up -d postgres-local redis-local",
    "db:local:down": "docker-compose -f docker-compose.local.yml down",
    "db:local:reset": "docker-compose -f docker-compose.local.yml down -v && bun run db:local:up",
    "db:reset": "./scripts/db-reset.sh",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:local": "APP_ENV=local drizzle-kit migrate",
    "db:migrate:dev": "APP_ENV=development drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:push:local": "APP_ENV=local drizzle-kit push",
    "db:push:dev": "APP_ENV=development drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:studio:local": "APP_ENV=local drizzle-kit studio",
    "db:studio:dev": "APP_ENV=development drizzle-kit studio",
    "healthcare:setup": "bun run scripts/setup-healthcare-local.ts",
    "healthcare:setup:local": "APP_ENV=local DATABASE_URL=postgresql://myexpo:myexpo123@localhost:5432/myexpo_dev bun run scripts/setup-healthcare-local.ts",
    "healthcare:setup:dev": "APP_ENV=development bun run scripts/setup-healthcare-local.ts",
    "healthcare:setup:complete": "APP_ENV=local DATABASE_URL=postgresql://myexpo:myexpo123@localhost:5432/myexpo_dev bun run scripts/setup-healthcare-complete.ts",
    "// === NGROK & OAUTH SETUP ===": "",
    "ngrok:setup": "./scripts/setup-ngrok.sh",
    "ngrok:start": "./scripts/start-ngrok.sh",
    "oauth:env": "bun run scripts/generate-env-files.js",
    "oauth:test": "bun run scripts/test-oauth-minimal.ts",
    "oauth:test:server": "bun run scripts/test-oauth-standalone.ts",
    "oauth:reset": "bun run scripts/fix-oauth-users.ts",
    "// === DOCKER COMMANDS ===": "",
    "docker:up": "docker-compose up -d",
    "docker:up:build": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:reset": "./scripts/docker-reset.sh",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:api": "docker-compose logs -f api",
    "docker:logs:postgres": "docker-compose logs -f postgres",
    "// === BUILD & DEPLOYMENT ===": "",
    "build:ios": "./scripts/eas-init.sh && eas build --platform ios --profile preview",
    "build:android": "bun run scripts/update-ip-address.js && ./scripts/eas-init.sh && eas build --platform android --profile preview",
    "build:all": "bun run scripts/update-ip-address.js && ./scripts/eas-init.sh && eas build --platform all --profile preview",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:android:production": "eas build --platform android --profile production",
    "// === TESTING & DEBUGGING ===": "",
    "test": "jest --watchAll",
    "test:auth": "./scripts/test-auth-flow.sh",
    "test:api": "bun run scripts/test-api-simple.ts",
    "api:health": "bun run scripts/check-api-health.ts",
    "debug:auth": "bun run scripts/test-auth-simple.ts",
    "debug:mobile": "bun run scripts/debug-mobile-auth.ts",
    "// === CODE QUALITY ===": "",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "bun run tsc --noEmit --skipLibCheck",
    "typecheck": "bun run tsc --noEmit --skipLibCheck",
    "clean": "rm -rf node_modules .expo dist-analysis",
    "clean:all": "rm -rf node_modules .expo dist-analysis ios/build android/build android/.gradle ios/Pods",
    "// === UTILITY SCRIPTS ===": "",
    "setup:env": "bun run scripts/setup-environment.sh",
    "fix:ios": "cd ios && pod install && cd ..",
    "fix:metro": "npx expo start --clear",
    "fix:imports": "bun run scripts/fix-imports.ts",
    "fix:console": "bun run scripts/remove-console-logs.ts",
    "credentials:setup": "./scripts/setup-ios-credentials.sh",
    "// === SERVER SCRIPTS ===": "",
    "server:dev": "bun run scripts/start-api-server.ts",
    "server:auth": "bun run scripts/start-auth-server.ts",
    "server:ws": "bun run scripts/start-websocket-server.ts",
    "server:email": "bun run scripts/start-email-server.ts"
  },
